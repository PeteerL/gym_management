<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.gymmanagement.controllers.ViewFeedbackController" spacing="10" alignment="CENTER">
    <Label text="Trainer Feedback" style="-fx-font-size: 18px; -fx-font-weight: bold;" />
    <TableView fx:id="feedbackTable" prefWidth="600" prefHeight="400">
        <columns>
            <TableColumn fx:id="feedbackTextColumn" text="Feedback Text" prefWidth="300" />
            <TableColumn fx:id="ratingColumn" text="Rating" prefWidth="150" />
            <TableColumn fx:id="timestampColumn" text="Timestamp" prefWidth="150" />
        </columns>
    </TableView>
</VBox>
